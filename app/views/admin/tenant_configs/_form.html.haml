= semantic_form_for @tenant_config do |f|
  = f.inputs do
    = f.input :credit_for, :label => "Credit Minutes for", :as => :select, :collection => {'PhoneScript' => 'PhoneScript', 'PhoneMenu' => 'PhoneMenu', 'Voicemail' => 'Voicemail'}, :include_blank => false
    = f.input :discount_minutes
  = f.actions