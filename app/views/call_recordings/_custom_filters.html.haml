%ul.dateRange
  %li Date Range:
  %li
    = collection_select(:call_records_search, :date, CustomFilter::DATE_RANGE, :first, :last, :include_blank => 'All', :selected => '')
    %input#search_date_from{name: "call_records_search[date_from]", type: "hidden"}/
    %input#search_date_to{name: "call_records_search[date_to]", type: "hidden"}/
  %li
    = link_to 'Custom Range', daterange_selector_call_recordings_path, remote: 'true', class: 'view fancybox fancybox.ajax'
