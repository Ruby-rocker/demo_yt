<% if @recordings.present? %>
	$('section.recordingsbodytable').html('<% call_recording_inner_records(@recordings) %>')
	$('#span_currenty_viewing').text('<%=@displaying_range %>')
<% else %>
	$('section.recordingsbodytable').html('<% call_recordings_no_records %>')
	$('#span_currenty_viewing').text('<%=@displaying_range%>')
<% end %>

