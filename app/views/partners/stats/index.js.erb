<% if @partner_state.present? %>
	$('section.tfoot.row').remove()
	$("<%= escape_javascript(render(:partial => 'partner_state_list', :locals => { partner_state: @partner_state, uniques: @uniques, commission: @commission })) %>").insertAfter('section.recordingsheadtable')
<% else %>
	$('section.tfoot.row').remove()
	$("<%= escape_javascript(render(:partial => 'partner_state_not_found')) %>").insertAfter('section.recordingsheadtable')
<% end %>

